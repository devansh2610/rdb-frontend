          "Filter APIs"
        ],
        "summary": "Get cocktail recipes by rating",
        "description": "This endpoint retrieves a list of cocktail recipes based on a rating threshold.",
        "operationId": "getCocktailRecipeByRating",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "threshold",
            "in": "query",
            "required": true,
            "type": "number",
            "description": "Minimum rating threshold",
            "default": "4.5",
            "collectionFormat": "single",
            "x-enum-values": [
              "4.0",
              "4.5",
              "5.0"
            ]
          },
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer",
            "description": "Page number",
            "default": "1",
            "collectionFormat": "single"
          },
          {
            "name": "page_size",
            "in": "query",
            "required": false,
            "type": "integer",
            "description": "Number of results per page",
            "default": "2",
            "collectionFormat": "single"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean"
                },
                "message": {
                  "type": "string"
                },
                "payload": {
                  "type": "array"
                },
                "Total_count": {
                  "type": "integer"
                }
              }
            }
          }
        },
        "security": [
          {
            "Authorization": []
          }
        ]
      }
    },
